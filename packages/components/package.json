    "jest-expo": "^35.0.0",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1",
    "react-test-renderer": "16.8.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
