  "main": "lib/index.js",
  "module": "src/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__",
    "src": "src"
  },
  "files": [
    "lib",
    "src"
  ],
  "publishConfig": {
    "access": "private"
  },
  "scripts": {
    "start": "nodemon --watch src --exec babel src --root-mode upward --out-dir lib --copy-files --source-maps --ignore '**/__tests__'",
    "build": "babel src --root-mode upward --out-dir lib --copy-files --source-maps --ignore '**/__tests__'",
    "test": "jest --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "test:develop": "jest -o --watch --coverage=false --changedSince=origin/master --detectOpenHandles",
    "test:updateSnapshots": "jest -u --coverage=false --detectOpenHandles",
    "postinstall": "expo-yarn-workspaces postinstall",
    "format": "prettier \"./**/*.{js,jsx,json,md}\" --write"
  },
  "dependencies": {
    "expo": "^35.0.0",
    "expo-barcode-scanner": "~7.0.0",
    "expo-constants": "~7.0.0",
    "expo-location": "~7.0.0",
    "expo-permissions": "~7.0.0",
    "prop-types": "^15.7.2",
